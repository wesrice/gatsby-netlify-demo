  "private": true,
  "scripts": {
    "build": "yarn gatsby clean && IS_PROD=true gatsby build && yarn util:mv",
    "deploy": "yarn --cwd ../infrastructure firebase deploy  --only hosting:website,storage:website",
    "deploy:ci": "yarn deploy --token \"$FIREBASE_TOKEN\"",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "tslint -c tslint.json 'src/**/*!(.scss.d).ts*'",
    "serve": "yarn --cwd ../infrastructure firebase serve --only hosting:website",
    "start": "gatsby develop",
    "util:mv": "rm -rf ../infrastructure/sites/static-site && mv public ../infrastructure/sites/static-site",
