    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-merge": "^4.2.2"
  },
  "fileSystemPath": "../",
  "name": "cms",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=development webpack --config webpack.prod.js && mv build ../infrastructure/sites/cms",
